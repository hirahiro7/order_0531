        .p-itemarea.is-max
          .p-itemarea__category
            .p-category
              .c-category.is-active ALL
              each cate , i in category
                .c-category !{cate.name}

          .p-itemarea__detail
        
            .p-item.is-active
              each item , i in items
                .c-item(data-recommend=`${i}` data-num=`${item.sku2}`)
                  .c-item__img
                    img(src="https://store.world.co.jp/img/item/BR688/BR68824S0026/BR68824S0026_l2_c102.jpg", alt="")
                  p.c-item__num !{item.sku1}-!{item.sku2}
                  p.c-item__name !{item.name}
                  p.c-item__price !{item.price}
                  .c-item__check
                    table.c-checkTbl
                      tr
                        td
                        each color, z in item.colors
                          th !{color}

                      each size, j in item.sizes
                        tr
                          td !{size.name}
                          each color, z in item.colors
                            td 
                              input(type="checkbox" data-size=`${size.name}` data-color=`${color}` name=`${size.val}` value=`${color}`).c-checkform.js-checkform
                  .c-item__other
                    .c-input
                      .c-input__label.is-other 補足情報
                      .c-input__txt
                        input(type="text" name=`${item.otherval}` placeholder="同アイテム2個以上等" disabled).c-textform.js-otherform

            each cate , i in category
              .p-item
                each item , i in items
                  .c-item(data-recommend=`${i}`)
                    .c-item__img
                    p.c-item__num !{item.sku1}-!{item.sku2}
                    p.c-item__name !{item.name}
